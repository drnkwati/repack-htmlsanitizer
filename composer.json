{
    "name": "drnkwati/repack-htmlsanitizer",
    "description": "Sanitize untrustworthy HTML input",
    "type": "library",
    "license": "MIT",
    "authors": [
    {
        "name": "Titouan Galopin",
        "email": "galopintitouan@gmail.com"
    },
    {
        "name": "Steve Nebes",
        "email": "snebes@gmail.com"
    },
    {
        "name": "Divine Nkwati",
        "email": "drnkwati@gmail.com"
    }],
    "require":
    {
        "php": ">=5.3",
        "ext-dom": "*",
        "ext-mbstring": "*",
        "masterminds/html5": "^2.0"
    },
    "require-dev":
    {
        "phpunit/phpunit": "^7.5"
    },
    "autoload":
    {
        "psr-4":
        {
            "Repack\\HtmlSanitizer\\": "src"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Tests\\Repack\\HtmlSanitizer\\": "tests"
        }
    },
    "suggest":
    {
        "psr/log": "Required for writing logs"
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev"
}